<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ucul_edit_and_btn_view">
        <attr name="hint_text" format="string"/>
        <attr name="text_size" format="dimension"/>
        <attr name="text_color" format="color"/>
        <attr name="edit_bg" format="reference"/>
        <attr name="btn_icon" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ucul_text_and_text_view">
        <attr name="tat_title_text" format="string"/>
        <attr name="tat_title_text_size" format="dimension"/>
        <attr name="tat_title_text_color" format="color"/>
        <attr name="tat_title_text_bg" format="reference"/>
        <attr name="tat_title_text_width" format="integer"/>
        <attr name="tat_division_bg" format="color"/>
        <attr name="tat_division_visibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="tat_content_text" format="string"/>
        <attr name="tat_content_text_size" format="dimension"/>
        <attr name="tat_content_text_color" format="color"/>
        <attr name="tat_content_text_bg" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ucul_text_and_edit_view">
        <attr name="tae_title_text" format="string"/>
        <attr name="tae_title_text_size" format="dimension"/>
        <attr name="tae_title_text_color" format="color"/>
        <attr name="tae_title_text_bg" format="reference"/>
        <attr name="tae_title_text_width" format="integer"/>
        <attr name="tae_division_bg" format="color"/>
        <attr name="tae_division_visibility">
            <enum name="visible" value="0" />
            <enum name="invisible" value="4" />
            <enum name="gone" value="8" />
        </attr>
        <attr name="tae_content_edit_hint_text" format="string"/>
        <attr name="tae_content_edit_text" format="string"/>
        <attr name="tae_content_edit_text_size" format="dimension"/>
        <attr name="tae_content_edit_text_color" format="color"/>
        <attr name="tae_content_edit_text_bg" format="reference"/>
    </declare-styleable>


</resources>